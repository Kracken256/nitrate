{
  "name": "quix",
  "displayName": "QUIX",
  "publisher": "qpkg",
  "description": "QUIX syntax highlighting support ",
  "version": "0.0.3",
  "repository": "https://github.com/Kracken256/quix/extensions/vscode/quix-syntax",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "quix",
      "aliases": ["QUIX", "quix"],
      "extensions": [".q",".qh"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "quix",
      "scopeName": "source.q",
      "path": "./syntaxes/quix.tmLanguage.json"
    }]
  }
}
