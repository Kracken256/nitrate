{
  "name": "nitrate-syntax",
  "displayName": "Nitrate: Syntax Highlighting",
  "publisher": "qpkg",
  "description": "Nitrate syntax highlighting support",
  "icon": "./icons/icon-light.png",
  "version": "0.0.10",
  "repository": "https://github.com/Kracken256/nitrate/extensions/vscode/nitrate-syntax",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nitrate",
        "aliases": [
          "Nitrate",
          "NITRATE",
          "nitrate"
        ],
        "extensions": [
          ".n"
        ],
        "icon": {
          "light": "./icons/icon-light.png",
          "dark": "./icons/icon-dark.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nitrate",
        "scopeName": "source.n",
        "path": "./syntaxes/nitrate.tmLanguage.json"
      }
    ]
  }
}