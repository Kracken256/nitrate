{
  "name": "quix-ir",
  "displayName": "QUIX-IR",
  "publisher": "qpkg",
  "description": "QUIX-IR syntax highlighting support ",
  "version": "0.0.2",
  "repository": "https://github.com/Kracken256/quix/extensions/vscode/qxir-syntax",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "quix-ir",
      "aliases": ["QUIX IR"],
      "extensions": [".qir"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "quix-ir",
      "scopeName": "source.qir",
      "path": "./syntaxes/quix-ir.tmLanguage.json"
    }]
  }
}
