{
  "name": "nitrate-ir",
  "displayName": "NITRATE-IR",
  "publisher": "qpkg",
  "description": "NITRATE-IR syntax highlighting support ",
  "version": "0.0.3",
  "repository": "https://github.com/Kracken256/quix/extensions/vscode/qxir-syntax",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nitrate-ir",
        "aliases": [
          "Nitrate IR"
        ],
        "extensions": [
          ".qir"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nitrate-ir",
        "scopeName": "source.qir",
        "path": "./syntaxes/nitrate-ir.tmLanguage.json"
      }
    ]
  }
}